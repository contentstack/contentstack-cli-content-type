      "flags": {
        "stack": {
          "name": "stack",
          "type": "option",
          "char": "s",
          "description": "Stack UID",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "stack-api-key": {
          "name": "stack-api-key",
          "type": "option",
          "char": "k",
          "description": "Stack API Key",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "token-alias": {
          "name": "token-alias",
          "type": "option",
          "char": "a",
          "description": "Management token alias",
          "multiple": false
        },
        "alias": {
          "name": "alias",
          "type": "option",
          "char": "a",
          "description": "Alias of the management token",
          "multiple": false
        },
        "order": {
          "name": "order",
          "type": "option",
          "char": "o",
          "description": "order by column",
          "multiple": false,
          "options": [
