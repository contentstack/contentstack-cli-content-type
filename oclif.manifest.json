        "$ csdx content-type:details --alias \"management token\" --content-type \"home_page\"",
        "$ csdx content-type:details --alias \"management token\" --content-type \"home_page\" --no-path"
      ],
      "flags": {
        "stack": {
          "name": "stack",
          "type": "option",
          "char": "s",
          "description": "Stack UID",
          "multiple": false,
          "exclusive": [
            "token-alias"
          ]
        },
        "stack-api-key": {
          "name": "stack-api-key",
          "type": "option",
          "char": "k",
          "description": "Stack API Key",
          "multiple": false,
          "exclusive": [
            "token-alias"
          ]
        },
        "token-alias": {
          "name": "token-alias",
          "type": "option",
          "char": "a",
          "description": "Management token alias",
          "multiple": false
        },
        "alias": {
          "name": "alias",
          "type": "option",
          "char": "a",
          "description": "Alias of the management token",
          "multiple": false
        },
        "content-type": {
          "name": "content-type",
          "type": "option",
          "char": "c",
          "description": "Content Type UID",
          "required": true,
          "multiple": false
        },
        "path": {
          "name": "path",
          "type": "boolean",
          "char": "p",
