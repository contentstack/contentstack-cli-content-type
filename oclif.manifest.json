      "args": {}
    },
    "content-type:diagram": {
      "id": "content-type:diagram",
      "description": "Create a visual diagram of a Stack's Content Types",
      "strict": true,
      "pluginName": "contentstack-cli-content-type",
      "pluginAlias": "contentstack-cli-content-type",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ csdx content-type:diagram --stack-api-key \"xxxxxxxxxxxxxxxxxxx\" --output \"content-model.svg\"",
        "$ csdx content-type:diagram --alias \"management token\" --output \"content-model.svg\"",
        "$ csdx content-type:diagram --alias \"management token\" --output \"content-model.svg\" --direction \"landscape\"",
        "$ csdx content-type:diagram --alias \"management token\" --output \"content-model.dot\" --type \"dot\""
      ],
      "flags": {
        "stack": {
          "name": "stack",
          "type": "option",
          "char": "s",
          "description": "Stack UID",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "stack-api-key": {
          "name": "stack-api-key",
          "type": "option",
          "char": "k",
          "description": "Stack API Key",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "token-alias": {
          "name": "token-alias",
          "type": "option",
          "char": "a",
          "description": "Management token alias",
          "multiple": false
        },
        "alias": {
          "name": "alias",
          "type": "option",
          "char": "a",
          "description": "Alias of the management token",
          "multiple": false
        },
        "output": {
          "name": "output",
          "type": "option",
          "char": "o",
          "description": "full path to output",
          "hidden": false,
          "required": true,
          "multiple": false
        },
        "direction": {
          "name": "direction",
          "type": "option",
          "char": "d",
          "description": "graph orientation",
          "hidden": false,
          "required": true,
          "multiple": false,
          "options": [
            "portrait",
            "landscape"
          ],
          "default": "portrait"
        },
        "type": {
          "name": "type",
          "type": "option",
          "char": "t",
          "description": "graph output file type",
          "hidden": false,
          "required": true,
          "multiple": false,
          "options": [
            "svg",
            "dot"
          ],
          "default": "svg"
        }
      },
      "args": {}
    },
    "content-type:list": {
      "id": "content-type:list",
