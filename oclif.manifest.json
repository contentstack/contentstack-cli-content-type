{
  "version": "1.2.0",
  "commands": {
    "content-type:audit": {
      "id": "content-type:audit",
      "description": "Display recent changes to a Content Type",
      "strict": true,
      "pluginName": "contentstack-cli-content-type",
      "pluginAlias": "contentstack-cli-content-type",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ csdx content-type:audit --stack-api-key \"xxxxxxxxxxxxxxxxxxx\" --content-type \"home_page\"",
        "$ csdx content-type:audit --alias \"management token\" --content-type \"home_page\""
      ],
      "flags": {
        "stack": {
          "name": "stack",
          "type": "option",
          "char": "s",
          "description": "Stack UID",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "stack-api-key": {
          "name": "stack-api-key",
          "type": "option",
          "char": "k",
          "description": "Stack API Key",
          "multiple": false,
          "exclusive": [
            "token-alias",
            "alias"
          ]
        },
        "token-alias": {
          "name": "token-alias",
          "type": "option",
          "char": "a",
          "description": "Management token alias",
          "multiple": false
        },
        "alias": {
          "name": "alias",
          "type": "option",
          "char": "a",
          "description": "Alias of the management token",
          "multiple": false
        },
        "content-type": {
          "name": "content-type",
          "type": "option",
          "char": "c",
          "description": "Content Type UID",
          "required": true,
          "multiple": false
        }
      },
      "args": {}
    },
    "content-type:compare-remote": {
      "id": "content-type:compare-remote",
      "description": "compare two Content Types on different Stacks",
      "strict": true,
      "pluginName": "contentstack-cli-content-type",
      "pluginAlias": "contentstack-cli-content-type",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ csdx content-type:compare-remote --origin-stack \"xxxxxxxxxxxxxxxxxxx\" --remote-stack \"xxxxxxxxxxxxxxxxxxx\" -content-type \"home_page\""
      ],
      "flags": {
        "origin-stack": {
          "name": "origin-stack",
          "type": "option",
          "char": "o",
          "description": "Origin Stack API Key",
          "required": true,
          "multiple": false,
          "dependsOn": [
            "remote-stack"
          ]
        },
        "remote-stack": {
          "name": "remote-stack",
          "type": "option",
          "char": "r",
          "description": "Remote Stack API Key",
          "required": true,
          "multiple": false,
          "dependsOn": [
            "origin-stack"
          ]
        },
        "content-type": {
          "name": "content-type",
          "type": "option",
          "char": "c",
          "description": "Content Type UID",
          "required": true,
          "multiple": false
        }
      },
      "args": {}
    },
    "content-type:compare": {
      "id": "content-type:compare",
      "description": "Compare two Content Type versions",
      "strict": true,
      "pluginName": "contentstack-cli-content-type",
      "pluginAlias": "contentstack-cli-content-type",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ csdx content-type:compare --stack-api-key \"xxxxxxxxxxxxxxxxxxx\" --content-type \"home_page\"",
        "$ csdx content-type:compare --stack-api-key \"xxxxxxxxxxxxxxxxxxx\" --content-type \"home_page\" --left # --right #",
        "$ csdx content-type:compare --alias \"management token\" --content-type \"home_page\" --left # --right #"
      ],
      "flags": {
        "stack": {
          "name": "stack",
          "type": "option",
          "char": "s",
          "description": "Stack UID",
          "multiple": false,
          "exclusive": [
            "token-alias"
          ]
        },
        "stack-api-key": {
          "name": "stack-api-key",
          "type": "option",
          "char": "k",
          "description": "Stack API Key",
          "multiple": false,
          "exclusive": [
            "token-alias"
          ]
        },
        "token-alias": {
          "name": "token-alias",
          "type": "option",
          "char": "a",
          "description": "Management token alias",
          "multiple": false
        },
        "alias": {
          "name": "alias",
          "type": "option",
          "char": "a",
          "description": "Alias of the management token",
          "multiple": false
        },
        "content-type": {
          "name": "content-type",
          "type": "option",
          "char": "c",
          "description": "Content Type UID",
          "required": true,
          "multiple": false
        },
        "left": {
          "name": "left",
          "type": "option",
          "char": "l",
          "description": "Content Type version, i.e. prev version",
          "multiple": false,
          "dependsOn": [
            "right"
          ]
        },
        "right": {
          "name": "right",
          "type": "option",
          "char": "r",
          "description": "Content Type version, i.e. later version",
          "multiple": false,
          "dependsOn": [
            "left"
          ]
        }
      },
      "args": {}
    },
